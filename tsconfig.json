{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
    "_helpers/*": ["_helpers/*"]
    },
    "target": "es2016",                                  
    "module": "commonjs",                                
    "forceConsistentCasingInFileNames": true,            
    "strict": false,                                     
    "skipLibCheck": true                               
  }
}